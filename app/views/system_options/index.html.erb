<h1>Listing system_options</h1>

<table>
  <tr>
    <th>Key</th>
    <th>Value</th>
  </tr>

<% @system_options.each do |system_option| %>
  <tr>
    <td><%=h system_option.key %></td>
    <td><%=h system_option.value %></td>
    <td><%= link_to 'Show', system_option %></td>
    <td><%= link_to 'Edit', edit_system_option_path(system_option) %></td>
    <td><%= link_to 'Destroy', system_option, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New system_option', new_system_option_path %>